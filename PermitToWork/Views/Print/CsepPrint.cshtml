                        <td style="text-align:center">
                            @(i == 0 ? (Model.screening_spv_arr[0] == "0" ? "N/A" : (Model.screening_spv_arr[0] == "1" ? "Yes" : (Model.screening_spv_arr[0] == "2" ? "No" : ""))) : 
