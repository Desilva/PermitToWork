                    <td class="td-centre">SPV</td>
                    <td class="td-centre">FO / PH</td>
                    @{
                        int i=0;
                        for (i = 0; i < 7;i++ )
                        {
                           <td class="td-centre">@(i+1)</td>
                        }    
                    }
                </tr>
